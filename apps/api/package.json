{
  "name": "api",
  "version": "1.3.0",
  "description": "API for LN Markets app on Umbrel",
  "main": "index.js",
  "author": "Kilian Botrel",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "eslint": "eslint --fix",
    "prettier": "prettier --write",
    "lint": "eslint --color 'src/**/*.js'",
    "format": "prettier --write 'src/**/*.js'",
    "dev": "nodemon"
  },
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "dependencies": {
    "@ln-markets/api": "1.7.0",
    "bech32": "2.0.0",
    "bip32": "3.1.0",
    "bip39": "3.0.4",
    "cors": "2.8.5",
    "cuid": "2.1.8",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "express-winston": "4.2.0",
    "helmet": "6.0.0",
    "ln-service": "54.2.3",
    "node-fetch": "3.2.10",
    "secp256k1": "4.0.3",
    "tiny-secp256k1": "2.2.1",
    "winston": "3.8.2",
    "ws": "8.6.0"
  },
  "nodemonConfig": {
    "exec": "node src/index.js",
    "watch": [
      "src/",
      ".env"
    ],
    "ext": "js"
  }
}
